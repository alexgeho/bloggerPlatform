#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandergerhard/Downloads/bloggerPlatform-main/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+types@29.6.3_jest@30.0.0_@types+node@24.0.0__typescript@5.8.3/node_modules/ts-jest/node_modules:/Users/alexandergerhard/Downloads/bloggerPlatform-main/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+types@29.6.3_jest@30.0.0_@types+node@24.0.0__typescript@5.8.3/node_modules:/Users/alexandergerhard/Downloads/bloggerPlatform-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandergerhard/Downloads/bloggerPlatform-main/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+types@29.6.3_jest@30.0.0_@types+node@24.0.0__typescript@5.8.3/node_modules/ts-jest/node_modules:/Users/alexandergerhard/Downloads/bloggerPlatform-main/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@7.27.4_@jest+types@29.6.3_jest@30.0.0_@types+node@24.0.0__typescript@5.8.3/node_modules:/Users/alexandergerhard/Downloads/bloggerPlatform-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
